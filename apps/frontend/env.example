# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_API_TIMEOUT=30000

# Authentication
VITE_JWT_STORAGE_KEY=rates_inventory_token
VITE_REFRESH_TOKEN_KEY=rates_inventory_refresh

# Application Settings
VITE_APP_NAME="Rates & Inventory Platform"
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AI_SUGGESTIONS=true
VITE_ENABLE_BULK_OPERATIONS=true
VITE_ENABLE_CHANNEL_SYNC=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_DEV_TOOLS=true

# Performance Settings
VITE_GRID_PAGE_SIZE=100
VITE_CACHE_TTL=300000
VITE_DEBOUNCE_DELAY=300

# UI Configuration
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_DATE_FORMAT=MM/dd/yyyy
VITE_DEFAULT_TIMEZONE=UTC

# Development Tools
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true
VITE_ENABLE_STORYBOOK=true
VITE_LOG_LEVEL=debug

# AG Grid License (for enterprise features)
# VITE_AG_GRID_LICENSE_KEY=your_ag_grid_enterprise_license_key_here

# Error Tracking (for production)
# VITE_SENTRY_DSN=your_sentry_dsn_here

# Analytics (for production)
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# CDN URLs (for production)
# VITE_STATIC_ASSETS_URL=https://cdn.rates-inventory.com 